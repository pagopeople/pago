{
    "type": "object",
    "properties": {
      "projectName": {
        "title": "Project name",
        "type": "string",
        "minLength": 1
      },
      "projectDescription": {
        "title": "Brief description of the project",
        "type": "string"
      },
      "companyGoal": {
        "title": "What company goal does this project work towards?",
        "type": "string",
        "enum": ["Goal 1", "Goal 2", "Goal 3", "Goal 4"]
      },
      "projectSize": {
        "title": "How big of a project is this?",
        "type": "string",
        "enum": ["Existential", "Large", "Medium", "Small"]
      },
      "hoursSpent": {
        "title": "How many hours did this take?",
        "type": "number"
      },
      "hadDeadline": {
        "title": "Did the project have a deadline?",
        "type": "boolean"
      },
      "deadlineDate": {
        "title": "What was the deadline?",
        "type": "string",
        "format": "date"
      },
      "managerInvolvementScore": {
        "title": "How involved was your manager?",
        "type": "integer"
      },
      "clearExpectationsScore": {
        "title": "Are expectations for the project clear?",
        "type": "integer"
      },
      "helpfulScore": {
        "title": "Has the work you have been doing been helpful in meeting company needs for the project?",
        "type": "integer"
      },
      "managerExcessiveScore": {
        "title": "Did your manager involve themselves too much",
        "type": "integer"
      },
      "withCoworkers": {
        "title": "Did you work with coworkers?",
        "type": "boolean",
        "format": "binary" 
      },
      "coworkerId": {
        "title": "Select coworker",
        "type": "string",
        "enum": ["Monte", "Spencer", "John", "Stephen"]
      },
      "coworkerPerformanceScore": {
        "title": "How did your coworker perform?",
        "type": "integer"
      },
      "skillsNeeded": {
        "title": "What skills have you needed or will you need on this project?",
        "type": "string",
        "enum": ["Math", "Coding", "People", "Knife", "Ninja"]
      },
      "skillScore": {
        "title": "What level of proficiency did you need for the skills selected?",
        "type": "integer"
      }, 
      "completionPercentage": {
        "title": "How close is this project to being finished",
        "type": "number",
        "minimum": 0,
        "maximum": 100,
        "default": 50
      }
    }  
  }